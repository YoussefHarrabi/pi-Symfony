{% extends 'base.html.twig' %}

{% block title %}Update Work{% endblock %}

{% block content %}
    <h1>Update Work</h1>

 {{ form_start(form) }}
        <div class="form-group">
              {{ form_row(form.location, {'attr': {'class': 'form-control'}}) }}
           
        </div>
        <div class="form-group">
           {{ form_row(form.startdate, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
           {{ form_row(form.enddate, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.description, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
        {{ form_row(form.image, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            <div class="form-check">
                {{ form_row(form.isactive, {'attr': {'class': 'form-check-input'}}) }}
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Update</button>
        {{ form_end(form) }}
{% endblock %}
